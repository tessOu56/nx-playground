{
  "name": "tech-stack-data",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/tech-stack-data/src",
  "projectType": "library",
  "tags": ["npm:private"],
  "// targets": "to see all targets run: nx show project tech-stack-data --web",
  "targets": {
    "collect": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run collect",
        "cwd": "libs/tech-stack-data"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/tech-stack-data",
        "tsConfig": "libs/tech-stack-data/tsconfig.lib.json"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libs/tech-stack-data"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/tech-stack-data/tsc",
        "main": "libs/tech-stack-data/src/index.ts",
        "tsConfig": "libs/tech-stack-data/tsconfig.typecheck.json",
        "assets": []
      }
    }
  }
}
