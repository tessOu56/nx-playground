{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@nx-playground/analytics": ["libs/analytics/src/index.ts"],
      "@nx-playground/animation-data": ["libs/animation-data/src/index.ts"],
      "@nx-playground/api-client": ["dist/libs/api-client"],
      "@nx-playground/api-server": ["apps/api-server/src"],
      "@nx-playground/auth": ["dist/libs/auth"],
      "@nx-playground/auth-client": ["dist/libs/auth-client/src"],
      "@nx-playground/charts": ["dist/libs/charts"],
      "@nx-playground/charts/chartjs": ["dist/libs/charts/chartjs"],
      "@nx-playground/charts/recharts": ["dist/libs/charts/recharts"],
      "@nx-playground/constants": ["libs/constants/src/index.ts"],
      "@nx-playground/design-system": ["dist/libs/design-system/src"],
      "@nx-playground/enterprise-admin": ["apps/enterprise-admin/src"],
      "@nx-playground/enterprise-data": ["libs/enterprise-data/src/index.ts"],
      "@nx-playground/error-handling": ["libs/error-handling/src/index.ts"],
      "@nx-playground/event-cms": ["apps/event-cms/src"],
      "@nx-playground/event-portal": ["apps/event-portal/src"],
      "@nx-playground/hooks": ["dist/libs/hooks/src"],
      "@nx-playground/i18n": ["dist/libs/i18n"],
      "@nx-playground/logger": ["libs/logger/src/index.ts"],
      "@nx-playground/permissions": ["libs/permissions/src/index.ts"],
      "@nx-playground/profile": ["apps/profile/src"],
      "@nx-playground/search-engine": ["libs/search-engine/src/index.ts"],
      "@nx-playground/supabase-client": ["libs/supabase-client/src/index.ts"],
      "@nx-playground/tech-stack-data": ["libs/tech-stack-data/src/index.ts"],
      "@nx-playground/test-utils": ["libs/test-utils/src/index.ts"],
      "@nx-playground/ui-components": ["dist/libs/ui-components"],
      "@nx-playground/utils": ["libs/utils/src/index.ts"],
      "@nx-playground/validation": ["libs/validation/src/index.ts"],
      "@nx-playground/vue-motion": ["apps/vue-motion/src"]
    }
  },
  "plugins": [
    {
      "name": "@vanilla-extract/typescript-plugin"
    }
  ],
  "exclude": ["node_modules", "tmp"]
}
